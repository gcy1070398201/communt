<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.exampl.communt.mapper.PublishMapper">
    <insert id="insert" parameterType="com.exampl.communt.mode.PublishMode">
        insert into publish
        (title,describe,label,creatId,replyCount,viewCount,gmt_create,gmt_modified)
        values (#{title},#{describe},#{label},#{creatId},#{replyCount},#{viewCount},#{gmtCreate},#{gmtModified})
    </insert>
    <select id="find">
        SELECT * FROM publish
    </select>
</mapper>